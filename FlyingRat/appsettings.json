{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "OrchardCore": {
    "OrchardCore.Setup": {
      "DefaultCulture": "zh-CN"
    },
    "OrchardCore.Admin": {
      "AdminUrlPrefix": "braksn"
    },
    "OrchardCore.Media": {
      "SupportedSizes": [ 16, 24, 32, 66, 80, 84, 100, 160, 180, 210, 220, 240, 250, 260, 280, 320, 330, 440, 480, 520, 600, 730, 850, 950, 1024, 2048 ],
      "UseTokenizedQueryString": false
    }
  },
  "IdentityOptions": {
    "Password": {
      "RequiredLength": 6,
      "RequiredUniqueChars": 1,
      "RequireNonAlphanumeric": false,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireDigit": false
    },
    "User": {
      "AllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-._@+"
      //"RequireUniqueEmail": true
    }
  }
}
