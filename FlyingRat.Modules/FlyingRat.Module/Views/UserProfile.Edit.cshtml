@model UserProfileViewModel
<div class="form-group">
    <label asp-for="NickName">昵称</label>
    <div class="input-group row col-md-4">
        <input asp-for="NickName" class="form-control" autofocus />
    </div>
</div>
<div class="form-group">
    <label asp-for="CreatedUtc">创建时间：@Model.CreatedUtc?.ToLocalTime().ToString("yyyy-MM-dd hh:mm:ss")</label>
</div>
<div class="form-group">
    <label asp-for="ModifiedUtc">修改时间：@Model.ModifiedUtc?.ToLocalTime().ToString("yyyy-MM-dd hh:mm:ss")</label>
</div>